{
  "name": "vue-basic-tooltip",
  "version": "1.0.0",
  "description": "a vuejs component library that displays simple tooltip",
  "main": "./dist/vue-basic-tooltip.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-basic-tooltip ./src/components/index.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isanbitoy/vue-basic-tooltip.git"
  },
  "keywords": [
    "vue",
    "simple",
    "tooltip"
  ],
  "author": "Lisandro A. Bitoy",
  "license": "MIT",
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^1.3.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    ".json",
    ".js"
